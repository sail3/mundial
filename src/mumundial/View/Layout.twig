  <body>
    <section class="user_acc_wrp" id="login_mu">
      <div class="gm_logo"></div>
      <article class="user_acc_bx">
        <h1 class="tit_sty01">REGISTRO</h1>
        <form action="/SignUp/Create?host=muonline.webzen.com" method="post">
          <input id="GameImage" name="GameImage" type="hidden" value="login_mu"/>
          <input id="SnsCode" name="SnsCode" type="hidden" value=""/>
          <input id="SnsID" name="SnsID" type="hidden" value=""/>
          <input id="SnsUserName" name="SnsUserName" type="hidden" value=""/>
          <input id="ReturnUrl" name="ReturnUrl" type="hidden" value=""/>
          <fieldset class="user_input_wrp">
            <legend class="join_title">Crear cuenta mumundial.com.</legend>
            <div class="login_box">
              <div class="input_div_wrp">
                <input placeholder="User-name" autocomplete="off" class="log_type" data-val="true" data-val-length="The username must consist of a minimum of 4 and a maximum of 20 characters. " data-val-length-max="20" data-val-length-min="4" data-val-notnumber="The username cannot contain only numbers." data-val-regex="The username must be a combination of letters and numbers." data-val-regex-pattern="^[a-zA-Z0-9]+$" data-val-remote="&amp;#39;UserID&amp;#39; is invalid." data-val-remote-additionalfields="*.UserID" data-val-remote-url="/SignUp/CheckUserID" data-val-required="Please enter a valid username." id="UserID" maxlength="20" name="UserID" style="width:298px;" type="text" value=""/>
              </div>
              <div class="input_div_wrp">
                <input placeholder="E-mail" autocomplete="off" class="log_type" data-val="true" data-val-length="The e-mail address may consist of a maximum of 65 characters." data-val-length-max="200" data-val-remote="&amp;#39;Email&amp;#39; is invalid." data-val-remote-additionalfields="*.Email" data-val-remote-url="/SignUp/CheckUserEmail" data-val-required="Please enter your e-mail address." data-val-webzenemail="The format of the e-mail address entered is incorrect." id="Email" maxlength="200" name="Email" style="width:298px;" type="text" value=""/>
              </div>
              <div class="input_div_wrp">
                <input placeholder="password" autocomplete="off" class="log_type" data-val="true" data-val-length="The password must consist of a minimum of 6 and a maximum of 20 characters." data-val-length-max="20" data-val-length-min="6" data-val-notcompare="Your username and password must be different." data-val-notcompare-otherproperty="UserID" data-val-notcontinue="Use of 4 consecutive letters or numbers is prohibited (e.g.: 1,2,3,4; ABCD)." data-val-notduplicated="You cannot use a password which has a duplicate character string of 4 or more characters from the username." data-val-notduplicated-otherproperty="UserID" data-val-notrepeat="Use of 4 of the same characters is prohibited (e.g.: 1111; AAAA)." data-val-regex="Passwords cannot contain certain special characters (e.g.: \ )." data-val-regex-pattern="^[0-9A-Za-z!\&quot;#$()*,.\/:;=?@\[\]\^_{|}~]+$" data-val-required="Please enter a valid password." data-val-strongpassword="Please enter a combination of letters, numbers and special characters (Some special characters cannot be used. E.g.: \)." id="Password" maxlength="20" name="Password" oncopy="return false" oncut="return false" style="width:298px;" type="password"/>
              </div>
              <div class="input_div_wrp">
                <input placeholder="re-password" autocomplete="off" class="log_type" data-val="true" data-val-equalto="The entered passwords do not match. Please re-enter this information." data-val-equalto-other="*.Password" data-val-required="Please re-enter password." id="RetypePassword" maxlength="20" name="RetypePassword" oncopy="return false" oncut="return false" style="width:298px;" type="password"/>
              </div>
              <div class="system_message alert"></div>
              <a onclick="signUp()" href="javascript:void(0)" id="submitButton" class="btn_login_bx">CREAR CUENTA</a>
            </div>
          </fieldset>
        </form>
      </article>
